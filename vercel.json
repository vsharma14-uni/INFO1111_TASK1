{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/php/*.php",
      "use": "vercel-php@0.6.0"
    }
  ],
  "routes": [
    {
      "src": "/php/(.*)",
      "dest": "/api/php/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NOW_PHP_DEBUG": "1"
  },
  "functions": {
    "api/php/*.php": {
      "runtime": "vercel-php@0.6.0"
    }
  }
}
