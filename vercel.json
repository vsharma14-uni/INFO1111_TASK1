{
  "version": 2,
  "functions": {
    "pages/api/redirect.js": {
      "maxDuration": 10,
      "memory": 128
    },
    "api/**/*.php": {
      "runtime": "vercel-php@1.0.0"
    }
  },
  "rewrites": [
    { "source": "/:path*.php", "destination": "/api/:path*.php" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "env": {
    "NOW_PHP_DEBUG": "1"
  }
}
